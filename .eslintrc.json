{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "globals": {
        "__CONFIG__": true
    },
    "rules": {
        "no-multiple-empty-lines": [
            2,
            {
                "max": 1
            }
        ],
        "no-unused-vars": [
            2,
            {
                "ignoreRestSiblings": true
            }
        ],
        "no-console": "warn",
        "no-debugger": "warn"
    }
}